{
  "name": "@f10r/workbench",
  "version": "0.0.0",
  "private": true,
  "browser": "build/workbench.js",
  "scripts": {
    "build": "rollup -c --experimentalDynamicImport",
    "dev": "rollup -c --watch --experimentalDynamicImport",
    "reload": "live-server --watch=build,src/html --open=src/html --mount=/js:./build  --mount=/js/@f10r/plugin:./node_modules/@f10r/plugin",
    "watch": "npm-run-all --parallel reload dev"
  },
  "dependencies": {
    "@f10r/lib": "^0.0.0",
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.58.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-watch": "^4.3.1"
  }
}
